function checkAll(field)
{
if (field.length==undefined){	// Only 1 checkbox
	if (!field.disabled){
		field.checked = true;
		} 
	}
else{
	for (i = 0; i < field.length; i++)
		if (!field[i].disabled){
			field[i].checked = true ;
		}
	}
}
function uncheckAll(field)
{
if (field.length==undefined){	// Only 1 checkbox
	field.checked = false;
	}
else{
	for (i = 0; i < field.length; i++)
		field[i].checked = false ;
	}
}
